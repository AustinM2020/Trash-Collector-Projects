<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>Customer Location</title>
    <style>
        #map{
            height: 400px;
            width: 100%;
        }
    </style>
</head>
<body>
    <div id="map"></div>
    <script>
        function initMap() {
            var location = { lat: 42, lng: -71 }
            var map = new google.maps.Map(document.getElementById('map'), { zoom: 8, center: location });

            var marker = new google.maps.Marker({ position: location, map: map })
        }
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDSJs7_8WHzvait6LVfq-wkqdV20Xx_lSQ&callback=initMap">
    </script>
    <script src="https://maps.googleapis.com/maps/api/geocode/json?address=1600+Amphitheatre+Parkway,+Mountain+View,+CA&key=AIzaSyDJz6GXgT6D1s33zJIec_bLlNKQGv_jXr8"></script>
    
</body>
</html>